;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bloxie Must Die"
run/main_scene="res://scenes/ui/main menu/main_menu_screen.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.png"

[autoload]

SceneManager="*res://addons/scene_manager/SceneManager.tscn"
SaveSystem="*res://scenes/autoloads/save_system.tscn"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
GameEvents="*res://scenes/autoloads/game_events.tscn"
GameManager="*res://scenes/autoloads/game_manager.tscn"
ObjectManager="*res://scenes/autoloads/object_manager.tscn"
ResourceHolder="*res://scenes/autoloads/resource_holder.tscn"
Utils="*res://scenes/utils/utils.tscn"
Shaker="*res://addons/shaker/src/Shaker.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Bloxie Must Die"

[editor_plugins]

enabled=PackedStringArray("res://addons/shaker/plugin.cfg", "res://addons/sound_manager/plugin.cfg", "res://addons/virtual_joystick/plugin.cfg")

[global_group]

player=""
effects_container=""
enemy_container=""
projectile_container=""
collectible_container=""
director=""

[gui]

theme/custom="res://resources/theme/game_theme.tres"
theme/custom_font="res://resources/fonts/Tomorrow_Night.ttf"
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0

